{
  "name": "filter-anything",
  "version": "2.1.0",
  "sideEffects": false,
  "description": "A simple (TypeScript) integration of \"pick\" and \"omit\" to filter props of an object",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "ava",
    "rollup": "rollup -c build/rollup.js",
    "build": "npm run lint && npm run lint && npm run test && npm run rollup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/filter-anything.git"
  },
  "keywords": [
    "pick",
    "omit",
    "typescript",
    "fillables",
    "guard",
    "filter",
    "wildcard",
    "props-stay",
    "filter-props",
    "delete-props",
    "guarded",
    "guarded-props",
    "filter-fields"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/filter-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/filter-anything#readme",
  "dependencies": {
    "is-what": "^3.6.0",
    "ts-toolbelt": "^6.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "ava": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "ava": {
    "extensions": ["ts"],
    "require": ["ts-node/register"]
  }
}
