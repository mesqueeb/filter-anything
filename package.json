{
  "name": "filter-anything",
  "version": "2.1.6",
  "sideEffects": false,
  "description": "A simple (TypeScript) integration of \"pick\" and \"omit\" to filter props of an object",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "ava",
    "rollup": "rollup -c build/rollup.js",
    "build": "npm run lint && npm run lint && npm run test && npm run rollup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/filter-anything.git"
  },
  "keywords": [
    "pick",
    "omit",
    "typescript",
    "fillables",
    "guard",
    "filter",
    "wildcard",
    "props-stay",
    "filter-props",
    "delete-props",
    "guarded",
    "guarded-props",
    "filter-fields"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/filter-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/filter-anything#readme",
  "dependencies": {
    "is-what": "^3.11.3",
    "ts-toolbelt": "8.0.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "ava": "^3.13.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^2.32.0",
    "rollup-plugin-typescript2": "^0.28.0",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  }
}
