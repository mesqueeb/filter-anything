{
  "name": "filter-anything",
  "version": "4.0.2",
  "description": "A simple (TypeScript) integration of \"pick\" and \"omit\" to filter props of an object",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint ./src",
    "test": "vitest run",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && npm run build && np"
  },
  "dependencies": {
    "is-what": "^5.2.0",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@cycraft/eslint": "^0.4.3",
    "@cycraft/tsconfig": "^0.1.2",
    "del-cli": "^6.0.0",
    "np": "^10.2.0",
    "vitest": "^3.0.6"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pick",
    "omit",
    "picked-props",
    "omited-props",
    "typescript",
    "fillables",
    "guard",
    "filter",
    "wildcard",
    "props-stay",
    "filter-props",
    "delete-props",
    "guarded",
    "guarded-props",
    "filter-fields"
  ],
  "author": "Luca Ban - Mesqueeb (https://cycraft.co)",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/filter-anything.git"
  },
  "homepage": "https://github.com/mesqueeb/filter-anything#readme",
  "bugs": "https://github.com/mesqueeb/filter-anything/issues"
}
